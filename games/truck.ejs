    const trucks = document.querySelectorAll('.jump');
    let object1;
    let object2;
    function jump(truck) {
        console.log("jump");
        truck.style.animation = 'none';
        setTimeout(() => {
            truck.style.animation = 'jump 0.8s';
        }, 10);
    }

    function createObject1() {
        object1 = document.createElement('div');
        object1.className = 'object1';
        object1.style.animation = 'none';

        const minTime = 3000;
        const maxTime = 8000;
        const randomTime = Math.floor(Math.random() * (maxTime - minTime + 1)) + minTime;

        setTimeout(() => {
            object1.style.animation = 'object1 ' + randomTime + 'ms';
        }, 10);

        document.body.appendChild(object1);

        const object1Interval = setInterval(() => {
            object1.remove();
            clearInterval(object1Interval);
        }, 3000);
    }
    setInterval(createObject1, 4000);

