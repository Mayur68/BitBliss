<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- <link rel="stylesheet" href="/frontend/global.css"> -->
    <title>truck</title>
    <style>
        body {
            margin: 0;
            padding: 0;
        }

        .loop-wrapper {
            margin: 0 auto;
            position: relative;
            display: block;
            width: 100%;
            height: 500px;
            overflow: hidden;
            border-bottom: 3px solid #fff;
            background-color: #15adff;
            border-bottom: 3px solid black;
        }

        .ground {
            width: 100%;
            height: 200px;
            background-color: #823d01;
        }

        .mountain {
            position: absolute;
            right: -900px;
            bottom: -20px;
            width: 2px;
            height: 2px;
            box-shadow:
                0 0 0 50px #979695,
                60px 50px 0 70px #979695,
                90px 90px 0 50px #979695,
                250px 250px 0 50px #979695,
                290px 320px 0 50px #979695,
                320px 400px 0 50px #979695;
            transform: rotate(130deg);
            animation: mtn 20s linear infinite;
        }

        @keyframes mtn {
            100% {
                transform: translateX(-3000px) rotate(130deg);
            }
        }

        .hill {
            position: absolute;
            right: -900px;
            bottom: -50px;
            width: 400px;
            border-radius: 50%;
            height: 20px;
            box-shadow:
                0 0 0 50px #009a3e,
                -20px 0 0 20px #009a3e,
                -90px 0 0 50px #009a3e,
                250px 0 0 50px #009a3e,
                290px 0 0 50px #009a3e,
                620px 0 0 50px #009a3e;
            animation: hill 4s 2s linear infinite;
        }

        @keyframes hill {
            100% {
                transform: translateX(-3100px);
            }
        }

        .tree,
        .tree:nth-child(2),
        .tree:nth-child(3) {
            position: absolute;
            height: 100px;
            width: 35px;
            bottom: 0;
            background: url(https://s3-us-west-2.amazonaws.com/s.cdpn.io/130015/tree.svg) no-repeat;
        }

        .tree {
            animation: tree 3s 0.000s linear infinite;
        }

        .tree:nth-child(2) {
            animation: tree2 2s 0.150s linear infinite;
        }

        .tree:nth-child(3) {
            animation: tree3 8s 0.050s linear infinite;
        }

        @keyframes tree {
            0% {
                transform: translate(1350px);
            }

            100% {
                transform: translate(-50px);
            }
        }

        @keyframes tree2 {
            0% {
                transform: translate(650px);
            }

            100% {
                transform: translate(-50px);
            }
        }

        @keyframes tree3 {
            0% {
                transform: translate(2750px);
            }

            100% {
                transform: translate(-50px);
            }
        }

        .rock {
            margin-top: -17%;
            height: 2%;
            width: 2%;
            bottom: -2px;
            border-radius: 20px;
            position: absolute;
            background: #ddd;
            animation: rock 4s -0.530s linear infinite;
        }

        .truck,
        .wheels {
            width: 85px;
            margin-right: -60px;
            bottom: 0px;
            right: 50%;
            position: absolute;
            background: #eee;
        }

        .truck {
            background: url(https://s3-us-west-2.amazonaws.com/s.cdpn.io/130015/truck.svg) no-repeat;
            background-size: contain;
            height: 60px;
            animation: truck 1s infinite;
        }

        .truck:before {
            content: " ";
            position: absolute;
            width: 25px;
            box-shadow:
                -30px 28px 0 1.5px #fff,
                -35px 18px 0 1.5px #fff;
            animation: wind 1s infinite;
        }

        .wheels {
            background: url(https://s3-us-west-2.amazonaws.com/s.cdpn.io/130015/wheels.svg) no-repeat;
            height: 15px;
            animation: truck 1s infinite;
        }

        @keyframes rock {
            0% {
                right: -200px;
            }

            100% {
                right: 2000px;
            }
        }

        @keyframes truck {
            6% {
                transform: translateY(0px);
            }

            7% {
                transform: translateY(-6px);
            }

            9% {
                transform: translateY(0px);
            }

            10% {
                transform: translateY(-1px);
            }

            11% {
                transform: translateY(0px);
            }
        }

        @keyframes jump {
            50% {
                transform: translateY(-150px);
            }

            100% {
                transform: translateY(0px);
            }
        }

        @keyframes wind {
            50% {
                transform: translateY(3px)
            }
        }

        .object1 {
            margin-top: -16%;
            height: 5%;
            width: 5%;
            border-radius: 20px;
            position: absolute;
            background: #1eff00;
        }

        @keyframes object1 {
            0% {
                transform: translate(1450px);
            }

            100% {
                transform: translate(-100px);
            }
        }

        .object2 {
            margin-top: -20%;
            height: 5%;
            width: 5%;
            border-radius: 20px;
            position: absolute;
            background: #ff0000;
        }

        @keyframes object2 {
            0% {
                transform: translate(1450px);
            }

            100% {
                transform: translate(-100px);
            }
        }
    </style>
</head>

<body>
    <div class="loop-wrapper">
        <div class="mountain"></div>
        <div class="hill"></div>
        <div class="tree"></div>
        <div class="tree"></div>
        <div class="tree"></div>
        <div class="rock"></div>
        <div class="truck jump"></div>
        <div class="wheels jump"></div>
    </div>
    <div class="ground"></div>
</body>
<script>
    // const scriptToRemove = document.getElementById("truck-script");
